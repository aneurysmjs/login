{
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "commit": "git-cz",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.18",
    "@tanstack/vue-query": "^4.32.0",
    "@tanstack/vue-table": "^8.9.3",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "@vueuse/core": "10.2.0",
    "axios": "^1.4.0",
    "lodash.debounce": "^4.0.8",
    "vee-validate": "^4.10.8",
    "vue": "3.3.4",
    "vue-router": "4.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.38.4",
    "@iconify-json/carbon": "1.1.16",
    "@iconify-json/svg-spinners": "^1.1.1",
    "@testing-library/vue": "^7.0.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "18.15.11",
    "@unocss/eslint-config": "0.51.4",
    "@unocss/preset-wind": "^0.51.13",
    "@unocss/reset": "0.51.4",
    "@unocss/transformer-directives": "^0.51.13",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue-macros/volar": "0.11.0",
    "@vue/test-utils": "2.4.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.38.0",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.1",
    "pnpm": "8.2.0",
    "simple-git-hooks": "2.8.1",
    "taze": "0.9.1",
    "typescript": "5.1.3",
    "unocss": "0.53.3",
    "unplugin-auto-import": "0.16.4",
    "unplugin-vue-components": "0.25.1",
    "unplugin-vue-macros": "2.3.0",
    "vite": "4.3.9",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-pages": "0.31.0",
    "vitest": "0.32.2",
    "vue-tsc": "1.8.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
