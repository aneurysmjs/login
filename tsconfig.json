{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "vite/client",
      "vite-plugin-pages/client",
      "unplugin-vue-macros/macros-global"
    ],
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "vueCompilerOptions": {
    "plugins": [
      "@vue-macros/volar/define-models",
      "@vue-macros/volar/define-slots"
    ]
  },
  "exclude": ["dist", "node_modules"]
}
